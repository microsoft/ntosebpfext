<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <!-- eBPF -->
    <eBPFForWindowsVersion>1.0.0-rc1</eBPFForWindowsVersion>
    <!-- Use NuGet global cache location for packages -->
    <NuGetPackageRoot Condition="'$(NuGetPackageRoot)' == ''">$(USERPROFILE)\.nuget\packages\</NuGetPackageRoot>
    <eBPFForWindowsPackagePath>$(NuGetPackageRoot)ebpf-for-windows.x64\$(eBPFForWindowsVersion)</eBPFForWindowsPackagePath>
  </PropertyGroup>
</Project>
